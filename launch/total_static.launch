	<!--GPS-->

	<!-- <include file="$(find ntrip_ros)/launch/ntrip_ros.launch"></include> -->
	<!-- <include file="$(find ntrip_ros_python3)/launch/ntrip_ros.launch"></include> -->

	<!--IMU-->
	<node pkg="myahrs_driver" type="myahrs_driver" name="myahrs_driver">
		<param name="port" value="/dev/ttyIMU" />
		<param name="baud_rate" value="115200" />
	</node>

	<!-- Nodes -->
	<node pkg="tricat231_pkg"  type="heading_converter.py" name="headingConverter" respawn="true"/>
    <node pkg="tricat231_pkg" type="gnss_converter.py" name="GnssConverter" respawn="true" output="screen"/>
	<node pkg="tricat231_pkg" type="lidar_clustering.py" name="LidarCluster" args="/dev/rplidar" respawn="true"/>
	<!-- <node pkg="tricat231_pkg" type="2.py" name="TotalStatic" output="screen"/> -->
	<node pkg="tricat231_pkg" type="total_static.py" name="TotalStatic" output="screen"/>
	<node pkg="tricat231_pkg" type="docking.py" name="Docking" respawn="true"/>
	<node pkg="tricat231_pkg" type="visual.py" name="Visual" respawn="true"/>
	<node pkg="rviz" type="rviz" name="visual_rviz" args="-d $(find tricat231_pkg)/rviz/tricat231.rviz"/>
	<!-- <node pkg="tricat231_pkg" type="serial_node.py" name="ARDUINO" args="/dev/ttyUSB1" respawn="true"/> -->

    <!-- Parameters -->
	<rosparam command="load" file="$(find tricat231_pkg)/params/231_params.yaml"/>
	<rosparam command="load" file="$(find tricat231_pkg)/params/lidar_params.yaml"/>

</launch>